<?xml version="1.0" encoding="utf-8"?>
<!--Linerlayout布局圆角化-->
<shape xmlns:android="http://schemas.android.com/apk/res/android" >
    <solid android:color="@color/colorWhite" />
    <stroke
        android:width="0.5dp"
        android:color="@color/colorGray" />

    <padding
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp" />

    <corners android:radius="10dp" />

</shape>